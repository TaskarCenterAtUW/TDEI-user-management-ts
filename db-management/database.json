{
    "defaultEnv": "pg",
    "pg": {
        "driver": "pg",
        "user": {
            "ENV": "DATABASE_USER"
        },
        "password": {
            "ENV": "DATABASE_PASSWORD"
        },
        "host": {
            "ENV": "DATABASE_HOST"
        },
        "database": {
            "ENV": "DATABASE_DB"
        },
        "port": {
            "ENV": "DATABASE_PORT"
        },
        "ssl": {
            "ENV": "SSL"
        }
    },
    "sql-file": true
}